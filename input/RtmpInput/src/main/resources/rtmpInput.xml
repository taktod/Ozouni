<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:p="http://www.springframework.org/schema/p"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
  <!-- writer設定 -->
  <bean id="writer" class="com.ttProject.ozouni.rtmpInput.ReceiveWriter"/>
  <!-- rtmpClient設定 -->
  <bean id="rtmpClient" class="com.ttProject.ozouni.rtmpInput.RtmpClient">
    <property name="objectEncoding" value="3"/>
    <property name="writerToSave" ref="writer"/>
  </bean>
</beans>
